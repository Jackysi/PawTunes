{
    "author": "Jaka Pra≈°nikar",
    "name": "AIO Radio",
    "schemes": [
        {
            "compile": "scss/aio.light.scss",
            "name": "AIO Light",
            "style": "./css/aio.light.css"
        },
        {
            "compile": "scss/aio.dark.scss",
            "name": "AIO Dark",
            "style": "./css/aio.dark.css"
        }
    ],
    "size":     "720x355",
    "template": "template.html",
    "url": "https://prahec.com",
    "extra": {
        "playlistIconSize": {
            "label-full": false,
            "default": 32,
            "placeholder": 32,
            "name": "playlistIconSize",
            "class": "col-sm-3",
            "label": "Playlist(s) icon size",
            "type": "number",
            "reset": true
        },
        "disableArtworkSharing": {
            "default": false,
            "name": "disableArtworkSharing",
            "description": " Disable artwork hover and sharing function (Default: <i>Enabled</i>)",
            "type": "checkbox",
            "value": "true"
        }
    }
}